doctype html
html
  html(lang="en")
  head
        meta(charset="utf-8")
        meta(content="IE=edge,chrome=1", http-equiv="X-UA-Compatible")
        meta(name="viewport", content="width=device-width,initial-scale=1,maximum-scale=2,user-scalable=no")
        meta(name="description", content="Semantic-UI-Forest, collection of design, themes and templates for Semantic-UI.")
        meta(name="keywords", content="Semantic-UI, Theme, Design, Template")
        meta(name="author", content="PPType")
        meta(name="theme-color", content="#ffffff")
        title Dashboard Template for Semantic-UI
        link(rel="stylesheet", href="/css/semantic.css", type="text/css")
        style(type="text/css").
        
                body {
                -webkit-font-smoothing: antialiased;
                -moz-font-smoothing: grayscale;
                }
        
                #sidebar {
                position: fixed;
                height: 100vh;
                background-color: #f5f5f5;
                padding-top: 68px;
                padding-left: 0;
                padding-right: 0;
                }
        
                #sidebar .ui.menu > a.item {
                padding: 10px 20px;
                line-height: 20px;
                color: #337ab7;
                border-radius: 0 !important;
                margin-top: 0;
                margin-bottom: 0;
                }
        
                #sidebar .ui.menu > a.item.active {
                background-color: #337ab7;
                color: white;
                border: none !important;
                }
        
                #sidebar .ui.menu > a.item:hover {
                background-color: #eee;
                color: #23527c;
                }
        
                #content {
                padding-top: 56px;
                padding-left: 20px;
                padding-right: 20px;
                }
        
                #content h1 {
                font-size: 36px;
                }
        
                #content .ui.dividing.header {
                width: 100%;
                }
        
                .ui.centered.small.circular.image {
                margin-top: 14px;
                margin-bottom: 14px;
                }
        
                .ui.borderless.menu {
                box-shadow: none;
                flex-wrap: wrap;
                border: none;
                padding-left: 0;
                padding-right: 0;
                }
        
                .ui.mobile.only.grid .ui.menu .ui.vertical.menu {
                display: none;
                }
  body
    .ui.tablet.computer.only.padded.grid
      .ui.inverted.borderless.top.fixed.fluid.menu
        a.header.item Music Shop
    .ui.mobile.only.padded.grid
      .ui.top.fixed.borderless.fluid.inverted.menu
        a.header.item Music Shop
    .ui.padded.grid
      #sidebar.three.wide.tablet.only.three.wide.computer.only.column
        .ui.vertical.borderless.fluid.text.menu
          a.active.item Katigoriya yaratish
          a(href="/admin-product").item Maxsulot qo&apos;shish
          .ui.hidden.divider
      #content.sixteen.wide.mobile.thirteen.wide.tablet.thirteen.wide.computer.right.floated.column
        .ui.padded.grid
          .row
            h1.ui.huge.dividing.header Katigoriyalar
        <div class="ui form" style="padding-bottom: 15px">
            <div class="field">
                <label>Nomi</label>
                <input type="text" name="name" id="category-name" placeholder="Nomi">
            </div>
            <button class="ui button" type="submit" id="category-add">Qo'shish</button>
        </div>
          .row
            table.ui.single.line.striped.selectable.unstackable.table
              thead
                tr
                  th #
                  th Nomi 
                  th Maxsulotlar turi soni
              tbody#category-body
  script(src="/js/jquery.js") 
  script(src="/js/axios.js") 
  script(src="/js/main_admin1.js") 