            .row.px-xl-5
                each val, index in result.products
                    .col-lg-3.col-md-4.col-sm-6.pb-1
                        .product-item.bg-light.mb-4
                            .product-img.position-relative.overflow-hidden
                                img.img-fluid.w-100(src=`${val.img_url}`, alt=val.name style="height: 400px; object-fit: cover;")
